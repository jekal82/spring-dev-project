<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.domain.message.dao.MessageMapper">

	<select id="getMessage" resultType="com.example.demo.domain.message.dto.Message">
		SELECT 	NAME
		FROM 	MESSAGE
		WHERE 	ROWNUM = 1
	</select>
</mapper>
